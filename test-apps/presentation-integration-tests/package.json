  "scripts": {
    "build": "tsc 1>&2 && node ./node_modules/@bentley/build-tools/scripts/ignoreargs.js 1>&2",
    "build:watch": "tsc -w",
    "clean": "rimraf lib .rush",
    "docs": "npm run extract",
    "extract": "node ./node_modules/@bentley/build-tools/scripts/extract.js --fileExt=ts --extractFrom=./src --recursive --out=../../generated-docs/extract",
    "lint": "tslint --project . 1>&2",
    "test:integration": "mocha --opts ./mocha.opts ./lib/**/*.js",
    "test:integration:watch": "npm run test:integration -- --reporter min --watch-extensions ts --watch",
    "cover:integration": "npm run test:integration",
    "cover": "",
    "test": ""
  },
